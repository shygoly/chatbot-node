{
  "name": "chatbot-node",
  "version": "1.0.0",
  "description": "Lightweight Node.js proxy for chatbotadmin intelligent customer service APIs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Tests run via MCP Playwright\" && exit 0",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "chatbot",
    "proxy",
    "express",
    "coze",
    "shopify"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coze/api": "^1.3.7",
    "@coze/coze-js": "^0.2.0",
    "@prisma/client": "^6.18.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.13.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.3",
    "chart.js": "^4.5.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.18.0",
    "react-i18next": "^16.2.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "better-sqlite3": "^12.4.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
